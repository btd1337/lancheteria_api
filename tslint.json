{
  "defaultSeverity": "error",
  "extends": ["tslint:latest", "tslint-config-prettier"],
  "jsRules": {},
  "rules": {
    "quotemark": [true, "single"],
    "semicolon": [true, "always"],
    "noImplicitAny": true,
    "no-this-assignment": [true, {
      "allowed-names": ["^document$"]
    }],
    "only-arrow-functions": false
  },
  "rulesDirectory": []
}
